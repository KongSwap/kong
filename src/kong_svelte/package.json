{
  "name": "kong_svelte",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create kong_svelte && dfx deploy",
    "start": "vite",
    "dev": "vite dev",
    "prebuild": "npx @sveltejs/kit sync",
    "build": "vite build",
    "postbuild": "npx @sveltejs/kit sync",
    "check": "npx @sveltejs/kit sync && npx svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss,svelte}\"",
    "test": "vitest",
    "preview": "vite preview",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "copy-tv": "cp -r node_modules/tradingview/charting_library static/charting_library"
  },
