{
  "name": "kong_svelte",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create kong_svelte && dfx deploy",
    "start": "vite",
    "dev": "vite dev",
    "prebuild": "npx @sveltejs/kit sync",
    "build": "vite build",
    "postbuild": "npx @sveltejs/kit sync",
    "check": "npx @sveltejs/kit sync && npx svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss,svelte}\"",
    "test": "vitest",
    "preview": "vite preview",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "copy-tv": "cp -r node_modules/tradingview/charting_library static/charting_library"
  },
  "dependencies": {
    "@dfinity/agent": "^2.1.3",
    "@dfinity/auth-client": "^2.1.3",
    "@dfinity/candid": "^2.1.3",
    "@dfinity/identity": "^2.1.3",
    "@dfinity/principal": "^2.1.3",
    "@dfinity/utils": "^2.6.0",
    "apexcharts": "^4.1.0",
    "bignumber.js": "^9.1.2",
    "comlink": "^4.4.2",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.9",
    "lightweight-charts": "^4.2.1",
    "qrcode": "^1.5.4",
    "svelte-apexcharts": "^1.0.2",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.0",
    "svelte": "^4.0.0",
    "vite": "^4.3.0"
  }
}
