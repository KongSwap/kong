{
  "name": "kong_svelte",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create kong_svelte && dfx deploy",
    "start": "sh $(pwd)/scripts/generate_secrets.sh && vite",
    "prebuild": "sh $(pwd)/scripts/generate_secrets.sh && npx @sveltejs/kit sync",
    "build": "vite build",
    "postbuild": "npx @sveltejs/kit sync",
    "check": "npx @sveltejs/kit sync && npx svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss,svelte}\"",
    "test": "vitest --reporter=basic",
    "preview": "sh $(pwd)/scripts/generate_secrets.sh && vite preview",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "copy-tv": "cp -r node_modules/tradingview/charting_library static/charting_library"
  },
  "dependencies": {
    "@dfinity/agent": "^2.3.0",
    "@dfinity/auth-client": "2.3.0",
    "@dfinity/candid": "^2.3.0",
    "@dfinity/identity": "2.3.0",
    "@dfinity/ledger-icp": "2.6.10",
    "@dfinity/ledger-icrc": "^2.8.0",
    "@dfinity/nns": "8.3.1",
    "@dfinity/principal": "^2.3.0",
    "@dfinity/utils": "^2.10.1",
    "@dicebear/collection": "^9.2.2",
    "@dicebear/core": "^9.2.2",
    "@dicebear/identicon": "9.2.2",
    "@windoge98/plug-n-play": "0.0.61-siws-8",
    "bignumber.js": "9.1.2",
    "chart.js": "4.4.8",
    "chartjs-adapter-date-fns": "3.0.0",
    "comlink": "4.4.2",
    "d3": "7.9.0",
    "date-fns": "4.1.0",
    "emoji-picker-element": "^1.26.1",
    "html5-qrcode": "2.3.8",
    "hyperid": "3.3.0",
    "qrcode": "1.5.4",
    "three": "0.177.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "3.0.8",
    "@sveltejs/kit": "2.17.2",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.16",
    "@types/node": "^22.13.4",
    "@types/pouchdb-core": "7.0.15",
    "@types/qrcode": "1.5.5",
    "@types/three": "^0.176.0",
    "autoprefixer": "10.4.20",
    "dotenv": "16.4.7",
    "eslint": "^9.20.1",
    "eslint-plugin-svelte": "3.0.2",
    "jsdom": "26.0.0",
    "lodash-es": "4.17.21",
    "lucide-svelte": "0.486.0",
    "postcss": "8.5.3",
    "prettier": "3.5.1",
    "prettier-eslint": "16.3.0",
    "sass": "1.85.0",
    "svelte": "5.25.6",
    "svelte-language-server": "0.17.10",
    "svelte-portal": "2.2.1",
    "tailwindcss": "3.4.14",
    "terser": "5.39.0",
    "typescript": "5.8.2",
    "vite": "6.1.1",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-environment": "1.1.3",
    "vite-plugin-pwa": "0.21.1",
    "vitest": "3.0.6"
  }
}
